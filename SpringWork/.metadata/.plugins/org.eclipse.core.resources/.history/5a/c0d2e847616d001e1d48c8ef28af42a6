package basic06;

public class MessageBeanKr implements MessageBean {
	
	private String name;
	private int age;
	
	public void setName(String name) {this.name = name;}
	public void setAge(int age) {this.age = age;}



	public void sayHello(String name)  {
		System.out.println("안녕하세요~~" + name + "님");
	}
}
